{
  "manifest_version": 3,
  "name": "Spaced Repetition Language Learner",
  "version": "0.1.0",
  "description": "A Chrome extension to learn languages using spaced repetition.",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "src/background/index.js"
  },
  "options_page": "src/options/index.html",
  "action": {
    "default_title": "Spaced Repetition Language Learner"
  }
}
